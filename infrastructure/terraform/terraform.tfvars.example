# Copy to terraform.tfvars and fill in your values
# Never commit terraform.tfvars (contains secrets)

# DigitalOcean API token
# Generate at: https://cloud.digitalocean.com/account/api/tokens
do_token = "dop_v1_XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX"

# SSH public key contents (cat ~/.ssh/your_key.pub)
ssh_public_key = "ssh-ed25519 AAAA... user@hostname"

# Custom SSH port (any port > 1024)
ssh_port = 2277
