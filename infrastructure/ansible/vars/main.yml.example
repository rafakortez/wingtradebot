# Copy to vars/main.yml and fill in your values
# NEVER commit vars/main.yml (contains secrets)

# Custom SSH port (must match Terraform output)
ssh_port: 2277

# Application installation path on server
app_dir: /root/wingtradebot

# Git repository configuration
git_repo: https://github.com/rafakortez/wingtradebot.git
git_branch: main

# Node.js version
nodejs_version: "20"

# Swap file size in MB
swap_size_mb: 1024

# DuckDNS Domain (for Let's Encrypt SSL)
# Create at: https://www.duckdns.org
duckdns_domain: "yourbot.duckdns.org"
duckdns_token: "xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx"

# Dashboard Authentication Credentials (STATUS_AUTH)
dashboard_username: "admin"
dashboard_password: "YourStrongPasswordHere"

# SimpleFX API Credentials
simplefx_api_key: "your_api_key"
simplefx_api_secret: "your_api_secret"
