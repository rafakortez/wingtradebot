# SimpleFX API Configuration
# Copy this file to .env and fill in your actual API keys
SIMPLEFX_API_KEY=your_api_key_here
SIMPLEFX_API_SECRET=your_api_secret_here
SIMPLEFX_API_KEY2=
SIMPLEFX_API_SECRET2=

# Account Numbers
DEFAULT_ACCOUNT_NUMBER=3028761
DEFAULT_ACCOUNT_NUMBER2=

# Authentication
STATUS_AUTH_USERNAME=admin
STATUS_AUTH_PASSWORD=your_password_here
STATUS_AUTH2_USERNAME=admin
STATUS_AUTH2_PASSWORD=

# Server Configuration
PORT=443
DEBUG_LEVEL=normal
SERVER_IP=your_server_ip

# FastAPI Configuration
FASTAPI_URL=http://localhost:8000
FASTAPI_PORT=8000

# Flask Configuration
FLASK_PORT=5000
FLASK_SECRET_KEY=your_secret_key_here

# Django Configuration
DJANGO_PORT=8001
DJANGO_SECRET_KEY=your_django_secret_key_here
